FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "300/subsurface";
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            cyclic;
    }
    right
    {
        type            cyclic;
    }
    top
    {
        type            chesubsurfaceTop;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 0;
        YiFromSurf      uniform 0;
        value           uniform 0;
    }
    bottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


